{
  "name": "event-search",
  "version": "1.0.0",
  "description": "Modular TypeScript scraper for brewery/winery/restaurant event pages",
  "main": "dist/index.js",
  "bin": {
    "event-scrape": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "scrape": "ts-node src/cli/index.ts",
    "validate": "ts-node src/cli/index.ts validate",
    "test-scraper": "ts-node src/cli/index.ts test-scraper",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "scraper",
    "events",
    "brewery",
    "winery",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "ical-generator": "^6.0.0",
    "playwright": "^1.41.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
